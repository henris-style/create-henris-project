{
  "name": "create-henris-project",
  "version": "0.0.13",
  "description": "Create a new project using Henri's",
  "main": "cli.js",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "saofile.js",
    "template/"
  ],
  "scripts": {
    "test:cov": "npm run lint",
    "test": "npm run lint",
    "lint": "standard",
    "prepublishOnly": "npm run build",
    "build": "bili",
    "try": "rm -rf test-project && node cli.js test-project"
  },
  "repository": {
    "url": "henris-style/create-henris-project",
    "type": "git"
  },
  "author": "silvandiepen<me@silvandiepen.com>",
  "license": "MIT",
  "dependencies": {
    "await-exec": "^0.1.2",
    "sao": "^1.6.1"
  },
  "devDependencies": {
    "bili": "^4.8.1",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "prettier": "^1.15.2",
    "shelljs": "^0.8.3",
    "standard": "^12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js, !template/**": [
      "standard --fix",
      "git add"
    ],
    "*.{json,md}, !template/**": [
      "prettier --write",
      "git add"
    ]
  }
}
